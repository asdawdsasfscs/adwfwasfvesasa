		Lable_3.Selectable = true
		Lable_3.Size = UDim2.new(0, 283, 0, 38)
		Lable_3.Font = Enum.Font.SourceSans
		Lable_3.Text = labelText
		Lable_3.TextColor3 = Color3.fromRGB(255, 255, 255)
