        TabButton.Text = "Tab 1"
        TabButton.TextColor3 = Color3.fromRGB(143, 143, 143)
        TabButton.TextSize = 14.000

        Page.Name = "Page"
        Page.Parent = MainFrame
        Page.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        Page.BorderColor3 = Color3.fromRGB(60, 60, 60)
